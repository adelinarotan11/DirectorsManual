@model IEnumerable<DirectorsManual.Domain.Entities.TContract_detail>
@{
    ViewBag.Title = "Rez12StopP";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}



<div class="panel panel-default">
	<div class="panel-heading">
		<h3>Список договоров на покупку готового товара</h3>
	</div>
	<div class="panel-body">
		<table class="table table-striped table-condensed table-bordered">
			<tr>
			
				<th>ID предприятия</th>
				<th class="text-right">ID поставщика</th>
				<th class="text-right">Кол-во деталей</th>
				<th>Общая стоимость</th>
				<th class="text-right">Дата отгрузки</th>
				@*<th class="text-center">Действия</th>*@
			</tr>
			@foreach (var item in Model)
			{
			<tr>
		
				<td class="text-right">@item.CompanyID</td>
				<td class="text-right">@item.ProviderID</td>
				<td class="text-right">@item.Kolvo_Detail</td>
				<td class="text-right">@item.Price_detail.ToString("# грн")</td>
				<td class="text-right">@item.Date_delivery_detail</td>

			</tr>
			}
		</table>
	</div>
	<div class="panel-footer">
		@Html.ActionLink("Добавить деталь", "SecondStorageProcedure", null,
			new { @class = "btn btn-default" })
	</div>
</div>